{
  "name": "dcarbone/php-consul-api",
  "type": "library",
  "description": "PHP client for the Consul HTTP API",
  "homepage": "https://github.com/dcarbone/php-consul-api",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniel Carbone",
      "email": "daniel.p.carbone@gmail.com"
    }
  ],
  "keywords": [
    "consul",
    "consul-api",
    "php-consul-api"
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "dcarbone/gotime": "0.5.*",
    "dcarbone/gohttp": "v0.3.0",
    "guzzlehttp/guzzle": "~7",
    "guzzlehttp/psr7": "~2"
  },
  "autoload": {
    "files": [
      "src/Coordinate/funcs.php",
      "src/funcs.php"
    ],
    "psr-4": {
      "DCarbone\\PHPConsulAPI\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload-dev": {
    "files": [
      "src/Coordinate/funcs.php",
      "tests/funcs.php"
    ],
    "psr-4": {
      "DCarbone\\PHPConsulAPI\\": "src/",
      "DCarbone\\PHPConsulAPITests\\": "tests/"
    }
  }
}